<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - E7PN </title>
    <link rel="shortcut icon" type="image/x-icon" href="@Url.Content("~/FrontEnd/theme-assets/images/ico/favicon.ico")" />
    <link href="https://maxcdn.icons8.com/fonts/line-awesome/1.1/css/line-awesome.min.css" rel="stylesheet">
    <!-- BEGIN VENDOR CSS-->
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/FrontEnd/theme-assets/css/vendors.css")" />
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/FrontEnd/theme-assets/vendors/css/charts/chartist.css")" />
    <!-- END VENDOR CSS-->
    <!-- BEGIN CHAMELEON  CSS-->
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/FrontEnd/theme-assets/css/app-lite.css")" />
    <!-- END CHAMELEON  CSS-->
    <!-- BEGIN Page Level CSS-->
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/FrontEnd/theme-assets/css/core/menu/menu-types/vertical-menu.css")" />
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/FrontEnd/theme-assets/css/core/colors/palette-gradient.css")" />
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/FrontEnd/theme-assets/css/pages/dashboard-ecommerce.css")" />
    <link href="@Url.Content("~/Content/Site.css")" rel="stylesheet" />
    <!-- END Page Level CSS-->
    @*@Styles.Render("~/Content/css")
        @Scripts.Render("~/bundles/modernizr")*@

</head>
<body class="vertical-layout vertical-menu 2-columns   menu-expanded fixed-navbar" data-open="click" data-menu="vertical-menu" data-color="bg-chartbg" data-col="2-columns">
    <nav class="header-navbar navbar-expand-md navbar navbar-with-menu navbar-without-dd-arrow fixed-top navbar-semi-light">
        <div class="navbar-wrapper">
            <div class="navbar-container content">
                <div class="collapse navbar-collapse show" id="navbar-mobile">
                    <ul class="nav navbar-nav mr-auto float-left">
                        <li class="nav-item d-block ">
                            <a class="nav-link nav-menu-main menu-toggle " href="#"><i class="ft-menu"></i></a>
                        </li>
                    </ul>
                    <ul class="nav navbar-nav float-right">
                        <li class="nav-item d-none d-md-block">
                            <a class="nav-link nav-link-expand" href='~/Lonig/Login'>前台</a>
                        </li>
                        <li class="dropdown dropdown-user nav-item">
                            <a class="dropdown-toggle nav-link dropdown-user-link" data-toggle="dropdown"><span class="avatar avatar-online"><img src="~/FrontEnd/頭像.png" alt="avatar"><i></i></span></a>
                        </li>
                    </ul>
                </div>
            </div>
        </div>
    </nav>
    <div class="main-menu menu-fixed menu-light menu-accordion menu-shadow" data-scroll-to-active="true">
        <div class="navbar-header">
            <ul class="nav navbar-nav flex-row">
                <li class="nav-item mr-auto">
                    <a class="navbar-brand">
                        <img class="brand-logo" alt="Chameleon admin logo" src="~/FrontEnd/E7PN LOGO FINALLY2_logo small.svg" style="width:75px;" />
                        <h3 class="brand-text">E7PN</h3>
                    </a>
                </li>
                <li class="nav-item d-md-none">
                    <a class="nav-link close-navbar"><i class="ft-x"></i></a>
                </li>
            </ul>
        </div>
        <div class="main-menu-content">
            <ul class="navigation navigation-main" id="main-menu-navigation" data-menu="menu-navigation">
                <li class="nav-item">
                    <a href='@Url.Action("Index", "Home")'><i class="la la-signal"></i><span class="menu-title" data-i18n="">洞察報告</span></a>
                </li>
                <li class=" nav-item">
                    <a href='@Url.Action("users", "Home")'><i class="la la-user"></i><span class="menu-title" data-i18n="">會員管理</span></a>
                </li>
                @*<li class=" nav-item">
                    <a href='@Url.Action("Teams", "Home")'><i class="la la-users"></i><span class="menu-title" data-i18n="">小隊列表</span></a>
                </li>*@
                <li class=" nav-item">
                    <a href='@Url.Action("YearsSummary", "Home")'><i class="ft-layers"></i><span class="menu-title" data-i18n="">活動總表</span></a>
                </li>
                <li class=" nav-item">
                    <a href='@Url.Action("Gift", "Home")'><i class="la la-gift"></i><span class="menu-title" data-i18n="">獎品庫存</span></a>
                </li>
                <li class=" nav-item">
                    <a href='@Url.Action("EmegencyNotice", "Home")'><i class="la la-bullhorn"></i><span class="menu-title" data-i18n="">緊急通知</span></a>
                </li>
                <li class=" nav-item">
                    <a href='@Url.Action("ActivitySummary", "Home")'><i class="la la-file-pdf-o"></i><span class="menu-title" data-i18n="">歷年總表</span></a>
                </li>
            </ul>
        </div>
        <div class="navigation-background"></div>
    </div>
    <section class="content">
        <div class="container-fluid">
            @RenderBody()
        </div>
    </section>
    <footer class="footer footer-static footer-light navbar-border navbar-shadow">
        <div class="clearfix blue-grey lighten-2 text-sm-center mb-0 px-2">
            <span class="float-md-left d-block d-md-inline-block">&copy; @DateTime.Now.Year - Copyright </span>
        </div>
    </footer>

    <!-- BEGIN VENDOR JS-->
    <script src="@Url.Content("~/FrontEnd/theme-assets/vendors/js/vendors.min.js")" type="text/javascript"></script>
    <!-- BEGIN VENDOR JS-->
    <!-- BEGIN PAGE VENDOR JS-->
    <script src="@Url.Content("~/FrontEnd/theme-assets/vendors/js/charts/chartist.min.js")" type="text/javascript"></script>
    <!-- END PAGE VENDOR JS-->
    <!-- BEGIN CHAMELEON  JS-->
    <script src="@Url.Content("~/FrontEnd/theme-assets/js/core/app-menu-lite.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/FrontEnd/theme-assets/js/core/app-lite.js")" type="text/javascript"></script>
    <!-- END CHAMELEON  JS-->
    <!-- BEGIN PAGE LEVEL JS-->
    <script src="@Url.Content("~/FrontEnd/theme-assets/js/scripts/pages/dashboard-lite.js")" type="text/javascript"></script>
    <!-- END PAGE LEVEL JS-->
    <script src="@Url.Content("~/FrontEnd/theme-assets/checkbox.js")" type="text/javascript"></script>
    <!-- BEGIN PAGE LEVEL JS-->
    <!-- END PAGE LEVEL JS-->

    @RenderSection("scripts", required: false);

</body>
</html>